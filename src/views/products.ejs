<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Product.ejs</title>
</head>
<body>
    <h1>Hello World</h1>
    <% products.map((product)=>{ %>
      <h4>
        <a href="/product/<%=product.id%"><%=product.productTitle %></a>
      </h4>
      <p>
        <%=product.productDescription %>
      </p>
      <p>
        <%=product.productPrice %>
      </p>
    <%})%>

</body>
</html>